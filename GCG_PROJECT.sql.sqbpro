SELECT COUNT(*) FROM sod_session_start;
SELECT COUNT(*) FROM sod_revenue;
SELECT COUNT(*) FROM sod_adrevenue;
SELECT COUNT(*) FROM sod_battle_short;
SELECT 
    COUNT(DISTINCT user_id) AS day1_active_users
FROM sod_session_start
WHERE user_point_in_lifetime = 1;

SELECT 
    COUNT(DISTINCT user_id) AS day3_active_users
FROM sod_session_start
WHERE user_point_in_lifetime = 3;
SELECT 
    COUNT(DISTINCT user_id) AS day7_active_users
FROM sod_session_start
WHERE user_point_in_lifetime = 7;
SELECT
    ROUND(
        COUNT(DISTINCT CASE 
            WHEN ss.user_point_in_lifetime = 7 THEN ss.user_id 
        END) * 100.0
        / COUNT(DISTINCT r.user_id),
        2
    ) AS day1_retention_percent
FROM sod_register r
LEFT JOIN sod_session_start ss
    ON r.user_id = ss.user_id;
	
SELECT 
    r.user_country,
    ROUND(
        COUNT(DISTINCT CASE 
            WHEN ss.user_point_in_lifetime = 7 THEN r.user_id 
        END) * 100.0 
        / COUNT(DISTINCT r.user_id),
    2) AS day7_retention_percent
FROM sod_register r
LEFT JOIN sod_session_start ss
    ON r.user_id = ss.user_id
GROUP BY r.user_country
ORDER BY day7_retention_percent DESC;	
SELECT *
FROM sod_register
LIMIT 5;
SELECT 
    r.user_device_platform,
    COUNT(DISTINCT r.user_id) AS total_users,
    ROUND(
        COUNT(DISTINCT CASE 
            WHEN ss.user_point_in_lifetime = 7 THEN r.user_id 
        END) * 100.0
        / COUNT(DISTINCT r.user_id),
        3
    ) AS day7_retention_percent
FROM sod_register r
LEFT JOIN sod_session_start ss
    ON r.user_id = ss.user_id
GROUP BY r.user_device_platform
ORDER BY day7_retention_percent DESC;
SELECT
    DATE(event_time) AS install_date,
    COUNT(DISTINCT user_id) AS installs
FROM sod_register
GROUP BY DATE(event_time)
ORDER BY install_date;
SELECT
    DATE(event_time) AS revenue_date,
    SUM(revenue) AS total_revenue
FROM sod_revenue
GROUP BY DATE(event_time)
ORDER BY DATE(event_time);
SELECT *
FROM sod_revenue
LIMIT 5;
SELECT
    DATE(event_time) AS revenue_date,
    SUM(user_point_in_lifetime) AS total_revenue
FROM sod_revenue
GROUP BY DATE(event_time)
ORDER BY DATE(event_time);
SELECT
    r.user_device_platform,
    SUM(rv.user_point_in_lifetime) AS total_revenue
FROM sod_revenue rv
JOIN sod_register r
    ON rv.user_id = r.user_id
GROUP BY r.user_device_platform
ORDER BY total_revenue DESC;</sql><current_tab id="0"/></tab_sql></sqlb_project>
